{
    "manifest_version": 3,
    "name": "Search Observer",
    "version": "1.0.0",
    "description": "Monitors Google searches and sends them to backend",
    "permissions": [
      "tabs",
      "webNavigation"
    ],
    "host_permissions": [
      "https://www.google.com/*"
    ],
    "background": {
      "service_worker": "dist/background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon32.png"
    }
  }